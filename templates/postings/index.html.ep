% layout 'default';
% no strict;
%= include('postings/list');
<div class="posting_navigation">
    <div style="width: 20%">
% if ($page->{previous}) {
          <a href="<%= url_for({ page => page.previous }) %>">&lt;- Page <%= $page->{previous} %></a>
% }
    </div>
    <div style="text-align: right; width: 20%">
% if ($page->{next}) {
    <a href="<%= url_for({ page => $page->{next} }) %>">Page <%= $page->{next} %> -&gt;</a>
% }
    </div>
</div>
